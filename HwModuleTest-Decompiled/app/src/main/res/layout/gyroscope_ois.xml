        </TableRow>
        <TableRow android:gravity="center_vertical">
            <TextView android:textSize="20sp" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:background="@color/white" android:padding="2dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="1dp" android:text="@string/hw_self_dps" android:layout_weight="1"/>
