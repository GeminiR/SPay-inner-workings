        </TableRow>
    </TableLayout>
    <TextView android:textSize="30sp" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:id="@+id/ois_result" android:padding="2dp" android:paddingTop="60sp" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
